{
  "build": {
    "env": {
      "PRISMA_CLI_BINARY_TARGETS": "native,rhel-openssl-1.0.x"
    }
  },
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://cryptodonatios.vercel.app"
  },
  "installCommand": "npm install && npx prisma generate"
}